{
    "overrides": [
      {
        "files": ["**/*.ts", "**/*.js"],
        "options": {
          "parser": "typescript",
          "singleQuote": true,
          "printWidth": 120
        }
      },
      {
        "files": "**/*.scss",
        "options": {
          "parser": "css"
        }
      },
      {
        "files": "**/*.html",
        "options": {
          "parser": "angular"
        }
      },
      {
        "files": "**/*.json",
        "options": {
          "parser": "json",
          "printWidth": 120
        }
      }
    ]
  }
  