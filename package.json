{
  "name": "@starfishx/theme-hg",
  "version": "0.0.10",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json --port 4220",
    "build": "ng build --prod",
    "build:ssr": "ng run hg:server",
    "build:ssr-command": "cd ssr && tsc",
    "prepublish": "npm run build && npm run build:ssr",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "jest": {
    "preset": "jest-preset-angular",
    "testMatch": [
      "**/*.spec.ts"
    ],
    "transformIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/dist-server/",
      "/ssr/"
    ],
    "setupFiles": [
      "jest-date-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ]
  },
  "dependencies": {
    "@angular/animations": "^7.2.12",
    "@angular/common": "^7.2.12",
    "@angular/compiler": "^7.2.12",
    "@angular/core": "^7.2.12",
    "@angular/forms": "^7.2.12",
    "@angular/http": "^7.2.12",
    "@angular/platform-browser": "^7.2.12",
    "@angular/platform-browser-dynamic": "^7.2.12",
    "@angular/platform-server": "^7.2.12",
    "@angular/router": "^7.2.12",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "core-js": "^2.5.7",
    "date-fns": "^2.0.0-alpha.27",
    "glob": "^7.1.2",
    "ramda": "^0.25.0",
    "rxjs": "^6.2.0",
    "rxjs-compat": "^6.0.0-rc.0",
    "tslib": "^1.9.3",
    "zone.js": "^0.9.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular/cli": "^7.3.8",
    "@angular/compiler-cli": "^7.2.12",
    "@angular/language-service": "^7.2.12",
    "@types/jasmine": "~2.8.8",
    "@types/jest": "^24.0.25",
    "@types/node": "^8.10.60",
    "codelyzer": "^4.3.0",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^24.9.0",
    "jest-date-mock": "^1.0.8",
    "jest-preset-angular": "^8.0.0",
    "karma-cli": "~1.0.1",
    "prettier": "^1.16.4",
    "protractor": "^6.0.0",
    "ts-node": "~6.0.5",
    "tslint": "~5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "3.1.6"
  }
}
